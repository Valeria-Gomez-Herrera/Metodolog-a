Proceso doblaje
	//Paso 1: Ingresar un numero.
	Definir num Como Entero;
	Escribir "Ingresa un número";
	Leer num;
	//Paso 2: Multiplicar el numero por 2.
	Definir doblarnum Como Entero;
	doblarnum <- num * 2;
	//Paso 3: El resultado multiplicarlo por 25.
	Definir mult Como Entero;
	mult <- doblarnum * 25;
	//Paso 4: Mostrar el resultado.
	Escribir "El resultado es: ", mult;
FinProceso
